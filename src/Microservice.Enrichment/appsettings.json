{
  "ShowExceptions": "true",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "cache": "localhost:6379"
  },
  "CustomSwaggerUi": {
    "Personalised": true, // If you want to use a standard swagger.ui change to false
    "HeaderImg": "<img src='https://notitalamanca.blob.core.windows.net/allimages/header.svg' style='display:block; margin: 0px auto;' alt='not supported'>",
    "DocTitle": "Microservice.Enrichment",
    "PathCss": "/css/CustomSwaggerStyles.css"
  },
  "Aspire": {
    "StackExchange": {
      "Redis": {
        "ConfigurationOptions": {
          "ConnectTimeout": 3000,
          "ConnectRetry": 2
        },
        "DisableHealthChecks": false,
        "DisableTracing": false
      }
    }
  }
}
